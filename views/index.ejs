<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/style/index.css" type="text/css">
  <link href="https://fonts.googleapis.com/css2?family=Lato:wght@300;700&display=swap" rel="stylesheet">  <title>Vehicles</title>
</head>
<div class="title">
  <img id="logo" src="traffic-jam.png"><h2>All Vehicles</h2>
</div>
  <hr>
  <body>
    <div class="vehicle-container">

        <% if (!vehicles) { %>
          <div class="vehicle">No vehicles availiable</div>
        <% } else { %>

          <% vehicles.forEach(function(v) { %>
    <!-- Loops through vehicles arr of objs, creating own separate div containers-->
            <div class="vehicle"> 
              <p>Vehicle #: <%= v.id %></p>
              <strong><%= v.make %> <%= v.model %></strong><br>
              Color: <%= v.color %><br>
              <%= v.location_description %>
            </div>
          <% }) %>

        <% } %>
    </div>
  </body>
</html>